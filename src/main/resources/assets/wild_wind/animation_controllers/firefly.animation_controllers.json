{
	"format_version": "1.19.0",
	"animation_controllers": {
		"controller.animation.model.new": {
			"states": {
				"default": {
					"animations": ["glow"],
					"transitions": [
						{"active": "firefly.ticker() >= 60"}
					]
				},
				"active": {
					"animations": ["idle"],
					"transitions": [
						{"default": "query.hurt_time < 60"}
					]
				}
			}
		}
	}
}